<script>
    function getProductData(product) {
        var product = product;
        loadFile("./getProduct?product=" + product, function(result) {
            // fetch collection using passed in product ObjectID
            var data = JSON.parse(result);

            // Set details for the experience

            // Page title
            document.title = data[0].name + " | XR@ASU | Arizona State University"

            // Show video if it exists
             if (data[0].video != "") {	
				document.getElementById("videoheader").hidden = false;
				document.getElementById("vid").hidden = false;
				//document.getElementById("modalvid").hidden = false;
				
				document.getElementById("videoheader").src = data[0].video;
				//document.getElementById("modalvid").setAttribute("data-src",data[0].video);
             } 

            // Set hero image
            var hero = data[0].thumbnail.substr(0, data[0].thumbnail.indexOf('_small')) + ".png";
            document.getElementById("hero").style.backgroundImage = "url('" + hero + "'";

            // Link to the experience

            //If multiple URLs, dynamically add buttons
            if (data[0].urls != null && data[0].urls.length != 0) {
                for (var i = 0; i < data[0].urls.length; i++) {
                    var button = document.createElement('button');
                    if ((data[0].urls[i].url != "" && data[0].urls[i].url != null)) {
                        button.innerHTML = "Play Now <small>(" + data[0].urls[i].platform + ")</small>";
                        button.onclick = launchExperience.bind(this, [data[0].urls[i].url]);
                        button.classList.add('sectionTitle', 'playNowButton');
                    } else {
                        button.innerText = "Coming soon (" + data[0].urls[i].platform + ")";
                        button.style.backgroundColor = "#cdcdcd";
                        button.style.cursor = "default";
                    }

                    var playButtonsDesktop = document.getElementById('playButtonsDesktop');
                    playButtonsDesktop.appendChild(button);

                    var playButtonsMobile = document.getElementById('playButtonsMobile');
                    var button_mobile = button.cloneNode(true);
                    playButtonsMobile.appendChild(button_mobile);
                }
            } else if (data[0].url != "" && data[0].url != null) {
                // Experience only has one URL
                var button = document.createElement('button');
                button.innerText = "Play Now";
                button.onclick = launchExperience.bind(this, [data[0].url]);
                button.classList.add('sectionTitle', 'playNowButton');

                var playButtonsDesktop = document.getElementById('playButtonsDesktop');
                playButtonsDesktop.appendChild(button);

                var playButtonsMobile = document.getElementById('playButtonsMobile');
                var button_mobile = button.cloneNode(true);
                playButtonsMobile.appendChild(button_mobile);
            } else {
                // Experience has no URL
                var button = document.createElement('button');
                button.innerText = "Coming soon";
                button.classList.add('sectionTitle', 'playNowButton');
                button.style.backgroundColor = "#cdcdcd";
                button.style.cursor = "default";

                var playButtonsDesktop = document.getElementById('playButtonsDesktop');
                playButtonsDesktop.appendChild(button);

                var playButtonsMobile = document.getElementById('playButtonsMobile');
                var button_mobile = button.cloneNode(true);
                playButtonsMobile.appendChild(button_mobile);
            }



            // Text info
            document.getElementById("productname").innerHTML = data[0].name;
            document.getElementById("productDesc").innerHTML = data[0].description;
            document.getElementById("experienceType").innerHTML = data[0].experienceType;

            //alert(document.getElementById("modalvid").getAttribute("data-src"));
            document.getElementById("releasedate").innerHTML = data[0].releaseDate;
            document.getElementById("creationDate").innerHTML = data[0].creationDate;

            document.getElementById("createdBy").innerHTML = data[0].createdBy;
            document.getElementById("createdBy").setAttribute("href", data[0].creatorURL);

            document.getElementById("minRequirements").innerHTML = data[0].minimumRequirements;

            //Play Button will say "play beta" if Beta is in the title
            //var isBeta = data[0].name.toLowerCase().includes("beta");
            //if(isBeta)
            //document.getElementById("playButton1").innerHTML = "Play Beta";

            //Iterate through the credits array of objects
            for (var i = 0; i < data[0].credits.length; i++) {
                document.getElementById("creditsSection").innerHTML += "<div> <span class='creditLabels'>" + data[0].credits[i].title + " " + "</span>" + "<span class='creditNames'>" + data[0].credits[i].names + "</span>";
            }

            // show device compatibiliity
            console.log("compatible devices:" + data[0].compatibleDevices);
            if (data[0].compatibleDevices.includes("mobile")) {
                console.log("experience is compatible on mobile");
                document.getElementById("mobileDevice").style.display = "inline-block";

                // if (!deviceIsMobile) {
                //     document.getElementById("playButtonsDesktop").style.display = "none";
                //     document.getElementById("playButtonsMobile").style.display = "none";
                // }
            } else {
                console.log("experience is not compatible on mobile");
                document.getElementById("mobileDevice").style.display = "none";
                if (deviceIsMobile) {
                    document.getElementById("playButtonsDesktop").style.display = "none";
                    document.getElementById("playButtonsMobile").style.display = "none";
                }
            }
            if (data[0].compatibleDevices.includes("web")) {
                document.getElementById("webDevice").style.display = "inline-block";
            } else {
                document.getElementById("webDevice").style.display = "none";
            }
            if (data[0].compatibleDevices.includes("vr")) {
                document.getElementById("vrDevice").style.display = "inline-block";
            } else {
                document.getElementById("vrDevice").style.display = "none";
            }

            var compatibleDevicesRow = document.getElementById("compatibleDevices");
            var compatibleDevicesRowClone = compatibleDevicesRow.cloneNode(true);
            compatibleDevicesRowClone.id = "compatibleDevicesClone";
            var compatibleDevicesRowMobile = document.getElementById('compatibleDevicesMobile');
            compatibleDevicesRowMobile.appendChild(compatibleDevicesRowClone);
        });
    }

    function launchExperience(url, event) {
        // console.log(url);
        location.href = url
    }

    function parseURL(url) {

        var parser = document.createElement('a'),
            searchObject = {},
            queries, split, i;

        // Let the browser do the work
        parser.href = url;

        // Convert query string to object
        queries = parser.search.replace(/^\?/, '').split('&');
        for (i = 0; i < queries.length; i++) {
            split = queries[i].split('=');
            searchObject[split[0]] = split[1];
        }

        // alert(split[1]);
        //if (split[1] != null) {
        //    getProductData(split[1]);
        //}
        //else {
        getProductData(globalproduct); //"5f2e1b687ca85a5fef49f0c1");
        //}
    }

    $(document).ready(function() {
        // Gets the video src from the data-src on each button
        var $videoSrc;
        $(".video-btn").click(function() {
            $videoSrc = $(this).attr("data-src");
            console.log("button clicked" + $videoSrc);
        });

        // when the modal is opened autoplay it
        $("#myModal").on("shown.bs.modal", function(e) {
            $("#video").attr("src", $videoSrc);
            console.log("modal opened" + $videoSrc);
            // set the video src to autoplay and not to show related video. Youtube related video is like a box of chocolates... you never know what you're gonna get
            $("#video").attr(
                "src",
                $videoSrc + "?amp;showinfo=0&amp;modestbranding=1&amp;autoplay=1"
            );
        });

        // stop playing the youtube video when I close the modal
        $("#myModal").on("hide.bs.modal", function(e) {
            // a poor man's stop video
            $("#video").attr("src", "");
        });

    });
